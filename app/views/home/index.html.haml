.row.background-sky
  .col-lg-12
    .page-header
      %h1#type Laboratorios Analizate

.row
  /.col-lg-2
  .col-lg-12
    .bs-example
      #carousel-example-generic.carousel.slide.bs-docs-carousel-example
        %ol.carousel-indicators
          %li{"data-slide-to" => "0", "data-target" => "#carousel-example-generic"}
          %li{"data-slide-to" => "1", "data-target" => "#carousel-example-generic"}
          %li.active{"data-slide-to" => "2", "data-target" => "#carousel-example-generic"}
        .carousel-inner
          .item.active
            %img{alt: "First slide", :src => "/images/slide1.jpg", :height => "250"}/
          .item
            %img{:alt => "Second slide", :src => "/images/slide2.jpg"}/
          .item
            %img{:alt => "Third slide", :src => "/images/slide3.jpg"}/

        %a.left.carousel-control{"data-slide" => "prev", :href => "#carousel-example-generic"}
          %span.icon-prev
        %a.right.carousel-control{"data-slide" => "next", :href => "#carousel-example-generic"}
          %span.icon-next
  /.col-lg-2

.row
  .col-lg-12.background-sky
    .page-header
      %h1#type Quienes Somos
  / Headings
  .row
    .col-lg-4
      .bs-example
        %ul.nav.nav-tabs{:style => "margin-bottom: 15px;"}
          %li.active
            %a{"data-toggle" => "tab", :href => "#section1"} Mision
          %li
            %a{"data-toggle" => "tab", :href => "#section2"} Vision
          %li
            %a{"data-toggle" => "tab", :href => "#section3"} Nuestros Valores
        #myTabContent.tab-content
          #section1.tab-pane.fade.active.in
            %p
              Mision.
          #section2.tab-pane.fade
            %p
              Vision.
          #section3.tab-pane.fade
            %p
              Nuestros Valores.
    .col-lg-4
      %h2 Nuestros Servicios
      .bs-example
        %p.text-info Gasometria.
        %p.text-info Analisis de Sangre.
    .col-lg-4
      %h2 Encuentranos
      %p
        Estamos ubicados en

